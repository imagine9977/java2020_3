package sec2;
@FunctionalInterface
interface Lambda1{ //매게변수 x
	void method1();
	
}
@FunctionalInterface
interface Lambda2{ //매게변수 o
	void method2(int x, int y);
}
@FunctionalInterface
interface Lambda3{ //매게변수 x
	int method3();
	
}

@FunctionalInterface
interface Lambda4{ //매게변수 x
	int method4(int x, int y);
	
}
//Lambda: 메소드를 더 간소화한 표현식
public class LambdaExam {
	public static void main(String[] args) {
		Lambda1 f1 = new Lambda1() {
			/*
			@Override
			public void method1() {
				// TODO Auto-generated method stub
				int x= 100;
				int y =100;
				System.out.println(x+y);
			}
			*/
			Lambda1 f1 =() -> {
				int x =100, u=200;
				System.out.println(x+u);
			};
			
		};
	}
}
